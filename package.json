{
  "name": "StackticalBench",
  "version": "0.0.1",
  "description": "A Stacktical Platform solution that combines the Benchmarking and the Scalability Testing of your Stacktical Application.",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "main": "public/src/stacktical.js",
  "dependencies": {
    "bluebird": "^3.4.6",
    "ini": "^1.3.4",
    "lodash": "https://registry.npmjs.org/lodash/-/lodash-4.6.1.tgz",
    "moment": "^2.17.1",
    "request": "^2.72.0",
    "request-promise": "^4.1.1",
    "winston": "^2.3.0",
    "winston-syslog": "^1.2.5"
  },
  "scripts": {
    "pretest": "rm -rf ./tests/reports/ut ./tests/reports/coverage",
    "test": "istanbul cover _mocha ./tests -- --opts ./tests/mocha.opts",
    "posttest": "istanbul report html --dir ./tests/reports/coverage",
    "lint": "eslint src tests",
    "precheckstyle": "mkdir -p ./tests/reports/lint/",
    "checkstyle": "eslint -f checkstyle src tests > ./tests/reports/lint/report.xml"
  },
  "author": "Stacktical, Inc",
  "homepage": "https://stacktical.com",
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-date-string": "^0.1.0",
    "chai-json-schema": "^1.4.0",
    "eslint": "^3.14.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-node": "^3.0.5",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-multi": "^0.10.0",
    "mochawesome": "^2.0.2",
    "nock": "^9.0.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "supertest": "^2.0.1",
    "uuid": "^3.0.1"
  }
}
